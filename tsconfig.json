{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "dist",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true
  },
  "files": [
    "src/Avatar.ts",
    "src/Board.ts",
    "src/BoundingBox.ts",
    "src/Card.ts",
    "src/Client.ts",
    "src/Deck.ts",
    "src/DragAndDrop.ts",
    "src/Marble.ts",
    "src/Notepad.ts",
    "src/PrivateArea.ts",
    "src/Scene.ts"
  ]
}
